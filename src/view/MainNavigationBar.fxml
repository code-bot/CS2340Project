<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="50.0" prefWidth="600.0" styleClass="background-frame" stylesheets="@css/MainNavBar.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuController">
    <left>
        <MenuBar fx:id="pageMenu" prefHeight="48.0" prefWidth="130.0" styleClass="menu" stylesheets="@css/MainNavBar.css">
         <padding>
            <Insets top="10.0" />
         </padding></MenuBar>
    </left>
    <right>
        <Button alignment="CENTER" mnemonicParsing="false" onAction="#signOut" prefHeight="48.0" prefWidth="100.0" styleClass="nav-button" stylesheets="@css/MainNavBar.css" text="Sign Out" textAlignment="CENTER" />
    </right>
</BorderPane>
