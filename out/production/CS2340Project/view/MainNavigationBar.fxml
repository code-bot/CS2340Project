<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="50.0" prefWidth="600.0" styleClass="background-frame" stylesheets="@css/MainNavBar.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuController">
    <left>
        <Button alignment="CENTER" mnemonicParsing="false" prefHeight="48.0" styleClass="nav-button" stylesheets="@css/MainNavBar.css" text="Home" textAlignment="CENTER" />
    </left>
    <center>
        <Button alignment="CENTER" mnemonicParsing="false" prefHeight="48.0" styleClass="nav-button" stylesheets="@css/MainNavBar.css" text="Profile" textAlignment="CENTER" />
    </center>
    <right>
        <Button alignment="CENTER" mnemonicParsing="false" onAction="#signOut" prefHeight="48.0" styleClass="nav-button" stylesheets="@css/MainNavBar.css" text="Sign Out" textAlignment="CENTER" />
    </right>
</BorderPane>
