<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" styleClass="background-frame" stylesheets="@css/Register.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RegisterController">
   <children>

       <PasswordField fx:id="passwordField" layoutX="210.0" layoutY="167.0" promptText="Enter password..." styleClass="text-field" stylesheets="@css/Register.css" />
       <PasswordField fx:id="confirmPasswordField" layoutX="210.0" layoutY="206.0" promptText="Confirm password..." styleClass="text-field" stylesheets="@css/Register.css" />
       <TextField fx:id="emailField" layoutX="212.0" layoutY="114.0" promptText="Confirm email address..." styleClass="text-field" stylesheets="@css/Register.css" />
       <TextField fx:id="confirmEmailField" layoutX="212.0" layoutY="75.0" prefWidth="179.0" promptText="Enter email address..." styleClass="text-field" stylesheets="@css/Register.css" />
       <TextField fx:id="addressLine1Field" layoutX="209.0" layoutY="264.0" prefWidth="179.0" promptText="Enter Address Line 1" styleClass="text-field" stylesheets="@css/Register.css" />
       <TextField fx:id="zipcodeField" layoutX="209.0" layoutY="344.0" prefHeight="39.0" prefWidth="102.0" promptText="Zip Code" styleClass="text-field" stylesheets="@css/Register.css" />
       <ComboBox fx:id="stateComboBox" layoutX="312.0" layoutY="350.0" prefHeight="27.0" prefWidth="70.0" />
       <Button fx:id="register" ellipsisString="" layoutX="267.0" layoutY="466.0" mnemonicParsing="false" onAction="#registerUser" prefHeight="27.0" prefWidth="69.0" styleClass="register-button" stylesheets="@css/Register.css" text="Register" textFill="#005580" />
       <Hyperlink fx:id="cancelReg" alignment="CENTER" layoutX="267.0" layoutY="500.0" onAction="#cancel" prefHeight="25.0" prefWidth="69.0" styleClass="hyper-link" stylesheets="@css/Register.css" text="Cancel" />
       <ComboBox fx:id="userTypeBox" layoutX="214.0" layoutY="410.0" prefHeight="27.0" prefWidth="169.0" />
       <TextField fx:id="cityField" layoutX="209.0" layoutY="303.0" prefHeight="39.0" prefWidth="179.0" promptText="City" styleClass="text-field" stylesheets="@css/Register.css" />

   </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</AnchorPane>
